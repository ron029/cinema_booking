<% provide(:title, custom_title('Bookings')) %>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Cinema Name</th>
      <th>Movie Name</th>
      <th>Date</th>
      <th>Time</th>
    </tr>
  </thead>
  <tbody>
<% @book_user.each do |user| %>
    <tr>
      <td><%= user.cinema.name %></td>
      <td><%= user.movie.name %></td>
      <td><%= user.date %></td>
      <td><%= user.time %></td>
    </tr>
<% end %>
  </tbody>
</table>