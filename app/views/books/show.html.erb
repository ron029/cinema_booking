<% provide(:title, custom_title('Bookings')) %>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Cinema Name</th>
      <th>Movie Name</th>
      <th>Date</th>
      <th>Time</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= @cinema_name %></td>
      <td><%= @movie_name %></td>
      <td><%= @booking.date %></td>
      <td><%= Booking::times.key(@booking.time) %></td>
    </tr>
  </tbody>
</table>